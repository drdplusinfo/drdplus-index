{
  "name": "drdplus/index",
  "license": "MIT",
  "type": "page",
  "description": "List of Drd+ rules and related sites",
  "keywords": [
    "Drd plus",
    "rules",
    "sites"
  ],
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "/home/jaroslav/projects/drdplus/rules/rules-skeleton"
    }
  ],
  "require": {
    "php": ">=7.1",
    "drdplus/rules-skeleton": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "roave/security-advisories": "dev-master",
    "mockery/mockery": "~1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "pre-autoload-dump": [
      "rm -f DrdPlus/Tests/RulesSkeleton && ln -s ../../vendor/drdplus/rules-skeleton/DrdPlus/Tests/RulesSkeleton DrdPlus/Tests/"
    ]
  }
}
